#logs
  = link_to image_tag('rss.gif'), log_feed_url(current_game), :class => 'rss'
  
  %h3= 'Events'[]
  .list
    - @logs.each do |log|
      .log
        = log_link log
        
- content_for :head do
  = auto_discovery_link_tag :atom, log_feed_url(current_account), :title => 'Events for {account}'[:event_for_account, current_account.name]